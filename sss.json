{
    "module": {
        "rules": [
            {
                "test": {},
                "use": [
                    {
                        "loader": "D:\\react-webpack-boilerplate\\node_modules\\_extract-text-webpack-plugin@3.0.0@extract-text-webpack-plugin\\dist\\loader.js",
                        "options": {
                            "omit": 1,
                            "remove": true,
                            "publicPath": "../"
                        }
                    },
                    {
                        "loader": "style-loader"
                    },
                    {
                        "loader": "css-loader"
                    },
                    {
                        "loader": "less-loader"
                    },
                    {
                        "loader": "postcss-loader",
                        "options": {}
                    }
                ]
            },
            {
                "test": {},
                "exclude": {},
                "use": [
                    {
                        "loader": "babel-loader"
                    }
                ]
            },
            {
                "test": {},
                "use": [
                    "file-loader?limit=8192&name=images/[hash].[ext]"
                ]
            }
        ]
    },
    "devtool": "source-map",
    "plugins": [
        {
            "definitions": {
                "process.env": {
                    "NODE_ENV": "\"production\""
                }
            }
        },
        {
            "options": {
                "sourceMap": true,
                "compress": {
                    "warnings": false,
                    "drop_console": true
                }
            }
        },
        {
            "filename": "css/[name].css",
            "id": 1,
            "options": {
                "allChunks": true,
                "disable": false
            }
        },
        {
            "paths": [
                "js",
                "imgs"
            ],
            "options": {
                "root": "D:\\react-webpack-boilerplate\\dist",
                "verbose": true,
                "exclude": [
                    "css",
                    "favicon.ico"
                ],
                "allowExternal": false,
                "dry": false
            }
        },
        {
            "options": {
                "template": "./src/index.html",
                "filename": "index.html",
                "hash": false,
                "inject": true,
                "compile": true,
                "favicon": false,
                "minify": false,
                "cache": true,
                "showErrors": true,
                "chunks": "all",
                "excludeChunks": [],
                "title": "Webpack App",
                "xhtml": false
            }
        }
    ],
    "entry": {
        "app": "D:\\react-webpack-boilerplate\\src\\index.js",
        "lib": [
            "react",
            "react-dom"
        ]
    },
    "output": {
        "publicPath": "./",
        "path": "D:\\react-webpack-boilerplate\\dist",
        "filename": "js/[name].[hash].js",
        "chunkFilename": "js/[name].[chunkhash].js"
    },
    "resolve": {
        "extensions": [
            ".js",
            ".jsx",
            ".less",
            ".css"
        ]
    }
}